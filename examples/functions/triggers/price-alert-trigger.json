{
  "name": "neo-gas-price-alert",
  "description": "Triggers price alert function every 5 minutes",
  "type": "cron",
  "schedule": "*/5 * * * *",
  "function": "price-alert",
  "enabled": true,
  "retryPolicy": {
    "maxAttempts": 3,
    "initialDelay": "1m",
    "maxDelay": "5m",
    "backoffMultiplier": 2
  }
}