{
  "name": "neo-service-layer",
  "version": "1.2.0",
  "description": "A sophisticated service layer for the Neo N3 blockchain",
  "main": "dist/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build",
    "build:fast": "NEXT_TELEMETRY_DISABLED=1 next build --no-lint",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config config/jest/jest.config.js",
    "test:watch": "jest --config config/jest/jest.config.js --watch",
    "test:coverage": "jest --config config/jest/jest.config.js --coverage",
    "clean": "rimraf .next dist",
    "clean:cache": "rimraf .next/cache",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"docs/**/*.md\"",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "build:docs": "npm run docs:build",
    "build:all": "npm run build && npm run build:docs",
    "optimize": "npm run clean && npm run build:fast"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@cityofzion/neon-js": "^5.7.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/lab": "^7.0.0-beta.9",
    "@mui/material": "^7.0.0",
    "@tanstack/react-query": "^5.69.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/prismjs": "^1.26.5",
    "axios": "^1.3.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "css-loader": "^7.1.2",
    "ethers": "^6.13.5",
    "jsonwebtoken": "^9.0.2",
    "next": "^14.1.0",
    "next-themes": "^0.4.6",
    "prismjs": "^1.30.0",
    "prom-client": "^15.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "recharts": "^2.15.1",
    "style-loader": "^4.0.0",
    "supertest": "^6.3.1",
    "swr": "^2.1.0",
    "tailwindcss": "^3.3.0",
    "winston": "^3.8.0",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.2.19",
    "@types/supertest": "^6.0.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.0",
    "cors": "^2.8.5",
    "eslint": "^8.0.0",
    "eslint-config-next": "^13.5.11",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.0.0",
    "next-bundle-analyzer": "^0.6.7",
    "postcss": "^8.4.0",
    "prettier": "^2.8.0",
    "rimraf": "^4.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.3",
    "vitepress": "^1.0.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neo-project/neo-service-layer"
  },
  "author": "Neo Service Layer Team",
  "license": "MIT"
}
